apiVersion: v1
kind: Namespace
metadata:
  name: padman
metadata:
  name: nginx-deployment
  namespace: padman
spec:
  replicas: 2
  selector:
    matchLabels:
      app: nginx
  template:
    metadata:
      labels:
        app: nginx
    spec:
      containers:
      - name: nginx
        image: nginx
        ports:
        - containerPort: 80
